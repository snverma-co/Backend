{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/server.js" }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "GOOGLE_SERVICE_ACCOUNT_EMAIL": "@google_service_account_email",
    "GOOGLE_PRIVATE_KEY": "@google_private_key",
    "VITE_GOOGLE_MAPS_API_KEY": "@google_maps_api_key"
  }
}
